---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Seed Framework</title>
 <link href="http://www.seedframework.org/rss/atom.xml" rel="self"/>
 <link href="http://www.seedframework.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.seedframework.org/</id>
 <author>
   <name>Danny</name>
   <email>fungos@seedframework.org</email>
 </author>

 {% for post in site.posts limit:10 %}

 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.seedframework.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.seedframework.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>

 {% endfor %}
 
</feed>
